{
  "name": "inventory",
  "version": "1.0.0",
  "description": "Coparts Inventory ",
  "main": "index.js",
  "scripts": {
    "dev": "run-s build:types build:js runApp",
    "runApp": "nodemon ./dist/index.js",
    "type-check": "tsc  --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "run-s build:types build:js",
    "build:types": "tsc --emitDeclarationOnly -d",
    "build:js": "babel ./ --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "keywords": [
    "inventory"
  ],
  "author": "Calixte Simeon",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/app-root-path": "^1.2.4",
    "@types/mongodb": "^3.3.14",
    "@types/papaparse": "^5.0.3",
    "app-root-path": "^3.0.0",
    "convert-units": "^2.3.4",
    "csv": "^5.3.1",
    "csv-parse": "^4.8.5",
    "dotenv": "^8.2.0",
    "haversine-geolocation": "^1.3.0",
    "mongodb": "^3.5.2",
    "stream-transform": "^2.0.1"
  },
  "devDependencies": {
    "@types/convert-units": "^2.3.0",
    "@types/geojson": "^7946.0.7",
    "@types/node": "^13.5.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.5"
  }
}
